import java.util.*;
import java.io.*;

public class makeLake{
    private int[][]lake , int[][]instructions;
    private int landElevation;

    public makeLake(String fileName){
	Scanner scan = new Scanner(new File(fileName));
	lake = new int [scan.hasNextInt()][scan.hasNextInt()];
	landElevation = scan.hasNextInt();
	instructions = int[scan.hasNextInt()][3];
	